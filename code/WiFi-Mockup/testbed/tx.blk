#include <externals.blk>
#include "../transmitter/transmitter.blk"


fun comp amp() {
var y : arr[4] complex16;

repeat {
  (x : arr[4] complex16) <- takes 4;
  do {
     v_shift_left_complex16(y, x, 4);
  }
  emits y;
}
} in

read[bit] >>> transmitter() >>> amp() >>> write
 

