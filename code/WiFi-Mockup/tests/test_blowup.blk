fun comp left (len : int) {  
  times len seq {
    (x:arr[8] int) <- takes 8;
    emits x
  }
}

fun comp t1 () { 
  repeat seq {
    x <- takes 6;
    emit x[0]
  }
}

fun comp t2 () {
  repeat seq {
    var y : arr[64] int;
    x <- takes 48;
    emit y
  }
}


let comp main = read[int] >>> seq{ len <- take; (left(len) >>> t1()) >>> t2()} >>> write