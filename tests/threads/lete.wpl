
var w : int := 3;

fun f1(x:int){
  var y : int := 0;
  y := x;
  y := x + x * x;
  y := y * (x + 1);
  return (y % 7)
}

let comp main = 
seq { x <- do { w := f1(42); w := w + f1(43) ; return w; }
    ; fun g(r : int) {
             let z = f1(x+r);
             return z;
      };
      read >>> map g |>>>| map f1 >>> write 
    }

  