

let comp h1 = 
   seq { (x:int) <- take
       ; return [forceinline] x
       }

let comp h2 = 
   seq { (x:int) <- take
       ; return [autoinline] x
       }

let comp h3 = 
   seq { (x:int) <- take
       ; return [noinline] x
       }


let comp main
  = read >>> seq { y <- h1; z <- h2; w <- h3; return () } >>> write[int]





