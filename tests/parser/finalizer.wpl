fun foo(x : int) { 
    return (x+1)
} in

fun comp fin(c : ST (C int) int int) {
    seq { c >>> map foo
        ; emit 34
        }

} in

fun comp upstream() {
    seq { x <- takes 80
        ; emits x 
        ; do { return 4 }
        }
} in

--read[int] >>> write[int]

read[int] >>> fin(upstream()) >>> write[int]
