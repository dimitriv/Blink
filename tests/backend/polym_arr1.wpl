  var s : int := 0;
--  var al : arr[length(a)] int;
  for i in [0, length(a)] {
    s := s + a[i]
  };
  return s
} in 

fun test1() {
  var a1 : arr[2] int := {1,2};
  return sum(a1);
} in 

